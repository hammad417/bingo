                $("#chatBox").addChatboxMessage({
                  playerName: "Ik",
                  chatbox: chatboxes[$("#chatTabs li.active").attr("data-chatbox")],
                  message: message
                });

                $("#messageField").val("");
                $("#messageField").focus();

