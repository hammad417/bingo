                e.preventDefault();

                setTimeout((function(self, chatBoxes) {
                  return function() {
                    chatBoxes[$(self).parent().attr("data-chatbox")].reinitialise();
                  }
                })(this, chatboxes), 300);

                $(this).tab('show');
